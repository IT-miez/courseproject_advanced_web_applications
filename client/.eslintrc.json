{
    "env": {
      "browser": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "airbnb",
      "airbnb/hooks",
      "plugin:cypress/recommended"
    ],
    "rules": {
        "cypress/unsafe-to-chain-command": "off",
        "import/no-extraneous-dependencies": "off"
      }
  }